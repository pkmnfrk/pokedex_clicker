{
  "name": "poke-clicker",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@antimatter-dimensions/notations": "^3.2.0",
    "@babel/core": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^8.1.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "break_infinity.js": "^2.2.0",
    "browserslist": "^4.18.1",
    "camelcase": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^7.1.3",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "dotenv": "^17.2.4",
    "dotenv-expand": "^5.1.0",
    "eslint": "^8",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-resolve": "^30.2.0",
    "jest-watch-typeahead": "^1.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^11.1.3",
    "prompts": "^2.4.2",
    "react": "^19.2.4",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^19",
    "react-redux": "^9.2.0",
    "react-refresh": "^0.11.0",
    "redux": "^5.0.1",
    "resolve": "^1.22.11",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^16.0.7",
    "semver": "^7.3.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.0.2",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5",
    "webpack-dev-server": "^5.2.3",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "predeploy": "npm run build",
    "predeployprod": "SET PUBLIC_URL=https://pkmnfrk.github.io/pokedex_clicker&& npm run build",
    "predeploybeta": "SET PUBLIC_URL=https://pkmnfrk.github.io/pokedex_clicker/beta&& npm run build",
    "deployprod": "gh-pages -d build",
    "deploybeta": "gh-pages -d build -e beta",
    "deployhelp": "gh-pages --help"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": ["defaults"],
  "devDependencies": {
    "file-loader": "^6.2.0",
    "gh-pages": "^6.3.0"
  },
  "homepage": "https://pkmnfrk.github.io/pokedex_clicker",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
